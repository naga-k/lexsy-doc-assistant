# Vercel Postgres connection string (found in the Postgres dashboard under "Connection string").
POSTGRES_URL=postgresql://username:password@db-name.host.vercel-postgres.com:5432/dbname

# Token that grants read/write access to your Vercel Blob store (create under Storage > Blob access).
BLOB_READ_WRITE_TOKEN=pk_XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# OpenAI key for the LLM integrations used by the app.
OPENAI_API_KEY=sk-XXXXXXXXXXXXXXXXXXXXXXXX
